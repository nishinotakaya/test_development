<h1>記事投稿</h1>
<%= form_with(model: @article, local: true, multipart: true) do |f| %>

  <div class="title">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

    <div class="time">
    <%= f.label :scontent %>
    <%= f.text_area :content %>
  </div>

  <div class="content">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>

  <div class="submit">
    <%= f.submit %>
  </div>

<% end %>
